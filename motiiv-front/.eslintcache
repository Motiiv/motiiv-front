obal/btn_kakao.png';\nimport { createProfile, signUpKakao, login } from '../../modules/user';\n//import KaKaoLogin from \"react-kakao-login\";\n//import axios from \"axios\";\n\nconst { Kakao } = window;\n\nconst LoginBtn = styled.button`\n  cursor: pointer;\n  outline: none;\n  background: none;\n  border: none;\n`;\n\nfunction KaKao({ props }) {\n  const dispatch = useDispatch();\n\n  const Login = user => {\n    dispatch(login(user));\n  };\n\n  const saveKakaoInfo = user => {\n    dispatch(signUpKakao(user));\n  };\n\n  const onHandleLoginKaKao = () => {\n    Kakao.Auth.login({\n      success: authObj => {\n        Kakao.Auth.setAccessToken(authObj.access_token);\n        Kakao.API.request({\n          url: '/v2/user/me',\n          success: function (response) {\n            const user = {\n              snsId: response.id,\n              socialType: 'kakao',\n            };\n\n            //로그인 시도\n            Login(user);\n\n            //로그인 정보 없을 시 회원가입을 위해 정보 넘겨주기\n            const signin = {\n              username: response.kakao_account.profile.nickname,\n              profileImageUrl: response.kakao_account.profile.profile_image_url,\n              snsId: response.id,\n              socialType: 'kakao',\n            };\n            saveKakaoInfo(signin);\n          },\n          fail: function (error) {\n            console.log(error);\n          },\n        });\n        //props.history.push(\"/auth\");\n      },\n      fail: err => {\n        console.log(err);\n      },\n    });\n  };\n\n  return (\n    <LoginBtn\n      style={{ marginBottom: '1.6rem' }}\n      value=\"KaKao\"\n      onClick={onHandleLoginKaKao}\n    >\n      <img src={btnKakao} />\n    </LoginBtn>\n  );\n}\nexport default KaKao;\n","/Users/jungwook/Desktop/SOPT/SOPT 27기/motiiv-front/motiiv-front/src/components/common/Login/LoginInterestComponent.js",[],"/Users/jungwook/Desktop/SOPT/SOPT 27기/motiiv-front/motiiv-front/src/components/common/Like/Like.js",[],"/Users/jungwook/Desktop/S