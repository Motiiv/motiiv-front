\n            exact\n            path=\"/mymotiiv\"\n            render={props => <MyMotiiv props={props} />}\n          ></Route>\n        }\n        <Route\n          exact\n          path=\"/signup\"\n          render={props => <SignUp props={props} />}\n        ></Route>\n        {/* Setting */}\n        <Route\n          exact\n          path=\"/setting\"\n          render={props => <Setting props={props} />}\n        ></Route>\n        {/* Admin */}\n        <Route\n          exact\n          path=\"/admin\"\n          render={props => <Admin props={props} />}\n        ></Route>\n        {/* Detail */}\n        <Route\n          exact\n          path=\"/detail/:id\"\n          render={props => <Detail props={props} />}\n        ></Route>\n        {/* Upload */}\n        <Route\n          exact\n          path=\"/upload\"\n          render={props => <Upload props={props} />}\n        ></Route>\n      </Switch>\n      <SigninModal hideModal={hideModal} isShow={showLoginModalState} />\n      <FloatBtn workspaces={workspaces} isShow={onFloatBtn} />\n      <BottomBanner isShow={location.pathname != '/setting'} />\n      <Footer isShow={location.pathname != '/setting'} />\n      <MyNavBar loginState={loginState} tag={location.pathname}></MyNavBar>\n    </>\n  );\n}\n\nexport default App;\n","/Users/jungwook/Desktop/SOPT/SOPT 27기/motiiv-front/motiiv-front/src/style/theme.js",[],"/Users/jungwook/Desktop/SOPT/SOPT 27기/motiiv-front/motiiv-front/src/modules/index.js",[],"/Users/jungwook/Desktop/SOPT/SOPT 27기/motiiv-front/motiiv-front/src/pages/Detail/Detail.js",["290"],"/Users/jungwook/Desktop/SOPT/SOPT 27기/motiiv-front/motiiv-front/src/pages/SignUp/SignUp.js",[],"/Users/jungwook/Desktop/SOPT/SOPT 27기/motiiv-front/motiiv-front/src/pages/Upload/Upload.js",[],"/Users/jungwook/Desktop/SOPT/SOPT 27기/motiiv-front/motiiv-front/src/pages/Category/Category.js",["291"],"import React, { useEffect } from 'react';\nimport CategoryComponent from '../../components/Category/Categ