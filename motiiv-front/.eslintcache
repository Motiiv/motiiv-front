m;\n`;\nconst HeaderInfo = styled.div`\n  font-size: 2rem;\n  color: ${({ theme }) => theme.primary};\n  display: flex;\n  flex-direction: column;\n`;\nconst HeaderTitleText = styled.div`\n  font-size: 2rem;\n  color: #2cff2c;\n  font-weight: bold;\n`;\nconst HeaderSubText = styled.div`\n  font-size: 1.2rem;\n  margin-top: 0.5rem;\n  color: white;\n`;\nconst VideoText = styled.div`\n  font-size: 2.8rem;\n  color: white;\n  margin-top: 2.4rem;\n\n  overflow: hidden;\n  text-overflow: ellipsis;\n  word-wrap: break-word;\n  word-break: keep-all;\n  line-height: 2.8rem;\n  max-height: 6rem;\n  display: -webkit-box;\n  -webkit-line-clamp: 2;\n  -webkit-box-orient: vertical;\n  padding-top: 0.2rem;\n`;\nconst TagBox = styled.div`\n  display: flex;\n  width: 100%;\n`;\nfunction SwiperContent({ obj, choice, isNext, isPrev }) {\n  return (\n    <ContentWrapper\n      choice={choice}\n      idx={obj.idx}\n      isNext={isNext}\n      isPrev={isPrev}\n    >\n      <TextCard>\n        <HeaderInfo>\n          <HeaderTitleText>{obj.TextInfo.category}</HeaderTitleText>\n          <HeaderSubText>{obj.TextInfo.categoryTxt}</HeaderSubText>\n        </HeaderInfo>\n        <VideoText>{`${obj.TextInfo.videoTxt}`}</VideoText>\n        <TagBox>\n          {obj.TextInfo.hashTag.map((tag, idx) => (\n            <Tag\n              key={`tag-${idx}`}\n              hashTag={1}\n              color={({ theme }) => theme.primary}\n              text={obj.TextInfo.hashTag[idx]}\n              fontSize=\"1.2rem\"\n            ></Tag>\n          ))}\n        </TagBox>\n      </TextCard>\n      <VideoCard choice={choice} idx={obj.idx}>\n        <iframe\n          width=\"100%\"\n          style={{ height: '100%', width: '100%' }}\n          src={obj.VideoInfo.src}\n          frameBorder=\"0\"\n          allow=\"accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture\"\n          allowFullScreen\n        ></iframe>\n        <VideoRunningTime>{obj.VideoInfo.runningTime}</VideoRunningTime>\n      </VideoCard>\n    </ContentWrapper>\n  );\n}\nexport default SwiperContent;\n","/Users/jungwook/Desktop/SOPT/SOPT 27기/motiiv-front/motiiv-front/src/components/Button/MoreBtn.js",[],"/Users/jungwook/Desktop/SOPT/SOPT 27기/motiiv-front/motiiv-front/src/lib/api/user.js",[],"/Users/jungwook/Desktop/SOPT/SOPT 27기/motiiv-front/motiiv-front/src/lib/createRequestSaga.js",[],"/Users/jungwook/Desktop/SOPT/SOPT 27기/motiiv-front/motiiv-front/src/components/Tag/Tag.js",[],"/Users/jungwook/Desktop/SOPT/SOPT 27기/motiiv-front/motiiv-front/src/lib/api/client.js",[],{"ruleId":"119","replacedBy":"120"},{"ruleId":"121","replacedBy":"122"},{"ruleId":"123","severity":1,"message":"124","line":10,"column":8,"nodeType":"125","messageId":"126","endLine":10,"endColumn":20},{"ruleId":"123","severity":1,"message":"127","line":14,"column":27,"nodeType":"125","messageId":"126","endLine":14,"endColumn":33},{"ruleId":"123","severity":1,"message":"128","line":96,"column":24,"nodeType":"125","messageId":"126","endLine":96,"endColumn":37},{"ruleId":"123","severity":1,"message":"129","line":7,"column":9,"nodeType":"125","messageId":"126","endLine":7,"endColumn":20},{"ruleId":"123","severity":1,"message":"130","line":2,"column":23,"nodeType":"125","messageId":"126","endLine":2,"endColumn":36},{"ruleId":"123","severity":1,"message":"131","line":2,"column":38,"nodeType":"125","messageId":"126","endLine":2,"endColumn":51},{"ruleId":"132","severity":1,"message":"133","line":128,"column":9,"nodeType":"134","endLine":135,"endColumn":10},"no-native-reassign",["135"],"no-negated-in-lhs",["136"],"no-unused-vars","'SwiperBanner' is defined but never used.","Identifier","unusedVar","'Router' is defined but never used.","'setLoginState' is assigned a value but never used.","'goToPrivacy' is assigned a value but never used.","'ThemeConsumer' is defined but never used.","'ThemeProvider' is defined but never used.","jsx-a11y/iframe-has-title","<iframe> elements must have a unique titl