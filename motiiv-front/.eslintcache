ail */}\n        <Route\n          exact\n          path=\"/detail/:id\"\n          render={props => (\n            <Detail\n              props={props}\n              showModal={showModal}\n              isLoggined={loginState}\n            />\n          )}\n        ></Route>\n        {/* Upload */}\n        <Route\n          exact\n          path=\"/upload\"\n          render={props => <Upload props={props} />}\n        ></Route>\n      </Switch>\n      <SigninModal hideModal={hideModal} isShow={showLoginModalState} />\n      <FloatBtn workspaces={workspaces} isShow={onFloatBtn} />\n      <BottomBanner\n        isShow={\n          location.pathname !== '/setting' &&\n          location.pathname !== '/category/0'\n        }\n      />\n      <Footer isShow={location.pathname !== '/setting'} />\n      <MyNavBar isLoggined={loginState} tag={location.pathname}></MyNavBar>\n    </>\n  );\n}\n\nexport default App;\n","/Users/jungwook/Desktop/SOPT/SOPT 27기/motiiv-front/motiiv-front/src/modules/index.js",[],"/Users/jungwook/Desktop/SOPT/SOPT 27기/motiiv-front/motiiv-front/src/modules/user.js",[],"/Users/jungwook/Desktop/SOPT/SOPT 27기/motiiv-front/motiiv-front/src/modules/mymotiiv.js",[],"/Users/jungwook/Desktop/SOPT/SOPT 27기/motiiv-front/motiiv-front/src/pages/Upload/Upload.js",[],"/Users/jungwook/Desktop/SOPT/SOPT 27기/motiiv-front/motiiv-front/src/pages/Admin/Admin.js",[],"/Users/jungwook/Desktop/SOPT/SOPT 27기/motiiv-front/motiiv-front/src/pages/SignUp/SignUp.js",[],"/Users/jungwook/Desktop/SOPT/SOPT 27기/motiiv-front/motiiv-front/src/pages/Category/Category.js",["295"],"import React, { useEffect } from 'react';\nimport CategoryComponent from '../../components/Category/CategoryComponent';\nimport { getCategoryKeywords, getCategoryVideos } from '../../modules/video';\nimport { useDispatch, useSelector } from 'react-redux';\n\nfunction Category({ props,showModal,isLoggined }) {\n  const dispatch = useDispatch();\n\n  const {\n    keywords,\n    loading,\n    videos,\n 